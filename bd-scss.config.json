{
	"meta": {
		"name": "SoftX",
		"author": "Gibbu#1211",
		"version": "1.0.0",
		"description": "A soft and comfy feel for Discord.",
		"source": "https://github.com/DiscordStyles/SoftX",
		"invite": "ZHthyCw"
	},
	"compiler": {
		"prefix": true
	},
	"dev": {
		"target": ["src", "dev.scss"],
		"output": ["SoftX.theme.css"]
	},
	"dist": {
		"target": ["src", "dist.scss"],
		"output": ["dist", "SoftX.theme.css"]
	},
	"base": {
		"target": ["src", "base.scss"],
		"output": ["dist", "SoftX.css"]
	},
	"addons": [
		{
			"target": ["src", "addons", "_radialglow.scss"],
			"output": ["dist", "RadialGlow.css"]
		},
		{
			"target": ["src", "addons", "_verticaluserarea.scss"],
			"output": ["dist", "VerticalUserArea.css"]
		},
		{
			"target": ["src", "addons", "_serverrings.scss"],
			"output": ["dist", "ServerRings.css"]
		}
	]
}